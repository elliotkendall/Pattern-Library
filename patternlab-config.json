{
  "paths": {
    "root": "./",
    "styleguideDefault": "./node_modules/styleguidekit-assets-default/dist/",
    "source": {
      "root": "./src/",
      "patterns": "./src/_patterns/",
      "data": "./src/_data/",
      "meta": "./src/_meta/",
      "annotations": "./src/_annotations/",
      "styleguide": "./node_modules/styleguidekit-assets-default/dist/",
      "patternlabFiles": "./node_modules/styleguidekit-mustache-default/views/",
      "js": "./src/js",
      "php": "./src/php",
      "images": "./src/assets",
      "fonts": "./src/fonts",
      "icons": "./src/assets/icons",
      "logos": "./src/logos",
      "css": "./src/css/",
      "scss": "./src/scss/",
      "extensions": "./src/_extensions"
    },
    "public": {
      "root": "./public/",
      "patterns": "./public/patterns/",
      "data": "./public/styleguide/data/",
      "annotations": "./public/annotations/",
      "styleguide": "./public/styleguide/",
      "js": "./public/js/",
      "php": "./public/php/",
      "images": "./public/assets/",
      "fonts": "./public/fonts/",
      "icons": "./public/assets/icons/",
      "logos": "./public/logos/",
      "css": "./public/css/",
      "scss": "./public/scss/"
    },
    "dist": {
      "root": "./dist/",
      "js": "./dist/<%= pkg.version %>/js/",
      "images": "./dist/<%= pkg.version %>/assets/",
      "fonts": "./dist/<%= pkg.version %>/fonts/",
      "icons": "./dist/<%= pkg.version %>/assets/icons/",
      "logos": "./dist/<%= pkg.version %>/logos/",
      "css": "./dist/<%= pkg.version %>/css/",
      "php": "./dist/<%= pkg.version %>/php/"
    },
    "live": {
      "hostname": "template.library.emory.edu",
      "pathname": "/styleguide/patternlibrary/current"
    }
  },
  "styleGuideExcludes": [
    "templates",
    "pages"
  ],
  "defaultPattern": "all",
  "cleanPublic": true,
  "patternExtension": "hbs",
  "ignored-extensions": [
    "DS_Store"
  ],
  "ignored-directories": [],
  "debug": false,
  "ishControlsHide": {
    "s": false,
    "m": false,
    "l": false,
    "full": false,
    "random": false,
    "disco": false,
    "hay": true,
    "mqs": false,
    "find": false,
    "views-all": false,
    "views-annotations": false,
    "views-code": false,
    "views-new": false,
    "tools-all": false,
    "tools-docs": false
  },
  "ishMinimum": 240,
  "ishMaximum": 2600,
  "ishViewportRange": {
    "s": [240, 500],
    "m": [500, 800],
    "l": [800, 2600]
  },
  "patternStateCascade": [
    "specification",
    "construction",
    "review",
    "complete",
    "reconsideration",
    "deprecated"
  ],
  "patternExportPatternPartials": [],
  "patternExportDirectory": "./pattern_exports/",
  "baseurl": "",
  "cacheBust": true,
  "starterkitSubDir": "dist",
  "outputFileSuffixes": {
    "rendered": "",
    "rawTemplate": "",
    "markupOnly": ".markup-only"
  },
  "plugins": {
    "plugin-node-tab": {
      "enabled": true,
      "initialized": false,
      "options": {
        "tabsToAdd": [
          "scss",
          "css",
          "js",
          "json",
          "php"
        ]
      }
    },
    "plugin-node-engine-extender": {
      "enabled": true,
      "initialized": false
    }
  },
  "extensionPath": "src/_extensions/extensions.js"
}