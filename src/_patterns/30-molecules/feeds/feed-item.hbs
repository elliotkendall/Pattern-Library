<eul-feed-item inline-template :defaults="{{#unless use}}{{JSONstringify this}}{{else}}{{use}}{{/unless}}"{{#if foreach}} v-for="{{foreach}}" :key="item.id"{{/if}}>

  <div class="feed-item"
       :class="{'has-action': button.href}"
       :data-id="id">
    <span class="feed-item-date"><% date | decode %></span>
    <a class="link feed-item-title" :href="link.href" :target="link.target || '_self'"><% title | decode %></a>
    <a class="button -hollow feed-item-action" v-if="button.href" :href="button.href" :target="button.target || '_self'"><% button.label %></a>
  </div>

</eul-feed-item>
