{
  "title": "Archive of Events",
  "feed": "https://www.trumba.com/calendars/robert-woodruff-library.json",
  "model": {
    "id": "{eventID}",
    "date": {
      "start": "{startDateTime}",
      "end": "{endDateTime}",
      "formatted": "{dateTimeFormatted}"
    },
    "description": "{description}",
    "link": {
      "href": "{permaLinkUrl}"
    },
    "image?": {
      "criteria": "{eventImage} && {eventImage.url}",
      "value": {
        "src": "{eventImage.url}"
      }
    }
  },
  "button": {
    "href": "/patterns/60-templates-events-calendar/60-templates-events-calendar.html",
    "label": "View Events Calendar"
  }
}
