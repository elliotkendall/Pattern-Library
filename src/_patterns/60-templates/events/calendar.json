{
  "title": "Calendar of Events",
  "feed": "https://www.trumba.com/calendars/robert-woodruff-library.json",
  "model": {
    "id": "{eventID}",
    "date": {
      "start": "{startDateTime}",
      "end": "{endDateTime}",
      "formatted": "{dateTimeFormatted}"
    },
    "description": "{description}",
    "link": {
      "href": "/patterns/60-templates-events-event/60-templates-events-event.html?feed={__FEED__}&id={eventID}"
    },
    "image?": {
      "criteria": "{eventImage} && {eventImage.url}",
      "value": {
        "src": "{eventImage.url}"
      }
    }
  },
  "button": {
    "vue": true,
    "href": "'/patterns/60-templates-events-archive/60-templates-events-archive.html?feed=' + feed",
    "label": "View Events Archive"
  }
}
